{
  "manifest_version": 2,
  "name": "Darwin Import",
  "version": "0.0.1",
  "description": "Import iNaturalist and eBird data into Darwin",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon.png",
      "32": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
